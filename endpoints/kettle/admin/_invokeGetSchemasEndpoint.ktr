    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.lang.String;
import java.lang.Integer;
import java.net.URLEncoder;


// URL String
String URLInputField;
String URLOutputField;
String schemaField;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

	// Let's look up parameters only once for performance reason.
	//
	if (first) {
		URLInputField = getParameter("URL_INPUT_FIELD");
		URLOutputField = getParameter("URL_OUTPUT_FIELD");
		schemaField = getParameter("SCHEMA_FIELD");

		// Special Helper Flag of Kettle alerting us if this is the first pass through.
	    first=false;
	}

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
	//
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

	// Input variables
	String url_input = get(Fields.In, URLInputField).getString(r);
	String schema = get(Fields.In, schemaField).getString(r);
	
	// Output variables
	String tmp = URLEncoder.encode(schema);
	String url_output = url_input + tmp;

	get(Fields.Out, URLOutputField).setValue(outputRow, url_output);

    // putRow will send the row on to the default output hop.
	//
    putRow(data.outputRowMeta, outputRow);

	return true;
}]]></class_source>
