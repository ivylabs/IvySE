        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import com.ivyis.opensource.license.manager.LicenseDetails;
import com.ivyis.opensource.license.manager.LicenseManager;
import com.ivyis.opensource.license.manager.LicenseManagerFactory;
import java.util.Calendar;

private FieldHelper customerNameOut = null;
private FieldHelper emailOut = null;
private FieldHelper pluginNameOut = null;
private FieldHelper licenseKeyIn = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
		licenseKeyIn = get(Fields.In, "userLicense");
    }

	try{
		final LicenseManager lm = LicenseManagerFactory.getLicenseManager();
		final LicenseDetails licenseDetails = lm.decodeLicense(licenseKeyIn.getString(r));
	
		if(("ivyse").equalsIgnoreCase(licenseDetails.getPlugin()) && 
			licenseDetails.getCustomerName().equals(get(Fields.In, "userCustomerName").getString(r)) && 
			licenseDetails.getEmail().equals(get(Fields.In, "userCustomerEmail").getString(r))
		){
			get(Fields.Out, "Type").setValue(r, "Success");
			get(Fields.Out, "Message").setValue(r, "Valid license.");
		}else{
			get(Fields.Out, "Type").setValue(r, "Error");
			get(Fields.Out, "Message").setValue(r, "The license key file is invalid.");
		}

	}catch(Exception e){
		get(Fields.Out, "Type").setValue(r, "Error");
		get(Fields.Out, "Message").setValue(r, "Error on processing license file.");
	}

    r = createOutputRow(r, data.outputRowMeta.size());

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

    return true;
}
]]></class_source>
