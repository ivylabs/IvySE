    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.StringReader;
import java.io.PrintWriter;
import java.io.StringWriter;
import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import org.w3c.dom.CharacterData;
import org.w3c.dom.Document;
import org.w3c.dom.Element;
import org.w3c.dom.Node;
import org.w3c.dom.NodeList;
import org.xml.sax.InputSource;

private FieldHelper schemaXMLIn = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first) {
        first = false;
		schemaXMLIn = get(Fields.In, "schemaXML");
    }

	try{
    	final DocumentBuilder db = DocumentBuilderFactory.newInstance().newDocumentBuilder();
    	final InputSource is = new InputSource();
   		is.setCharacterStream(new StringReader(schemaXMLIn.getString(r)));
    	final Document doc = db.parse(is);
		get(Fields.Out, "Type").setValue(r, "Success");
		get(Fields.Out, "Message").setValue(r, "XML Schema valid.");
		get(Fields.Out, "MessageDetails").setValue(r, "");
		get(Fields.Out, "MessageFullDetails").setValue(r, "");
	}catch(Exception e){
		get(Fields.Out, "Type").setValue(r, "Error");
		get(Fields.Out, "Message").setValue(r, "XML Schema invalid.");
		get(Fields.Out, "MessageDetails").setValue(r, e.getMessage());
		get(Fields.Out, "MessageFullDetails").setValue(r, getStackTrace(e));
	}

    r = createOutputRow(r, data.outputRowMeta.size());

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

    return true;
}

public static String getStackTrace(Throwable throwable) {
	final StringWriter sw = new StringWriter();
	final PrintWriter pw = new PrintWriter(sw, true);
	throwable.printStackTrace(pw);
	return sw.getBuffer().toString();
}]]></class_source>
